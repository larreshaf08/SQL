
/** clothing:
jeans (1)
t-shirts (4)
jogging pants (2)
shorts (1)
socks (5)
sweater (2)
**/


CREATE TABLE clothing (id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, price INTEGER, asile INTEGER);

INSERT INTO clothing VALUES (1, "jeans", 1, 5, 6);
INSERT INTO clothing VALUES (2, "t-shirts", 4, 3, 7);
INSERT INTO clothing VALUES (3, "jogging pants", 2, 10, 8);
INSERT INTO clothing VALUES (4, "shorts", 1, 5, 9);
INSERT INTO clothing VALUES (5, "socks", 5, 3, 10);
INSERT INTO clothing VALUES (6, "sweater", 2, 10, 11);

SELECT * FROM clothing WHERE asile < 9 ORDER BY price;
